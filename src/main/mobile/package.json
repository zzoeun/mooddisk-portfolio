{
  "name": "mobile",
  "main": "index.ts",
  "version": "1.0.2",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:clean": "adb uninstall com.mooddisk.mobile 2>/dev/null || true && expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build:android": "NODE_ENV=production expo run:android --device",
    "build:android:release": "NODE_ENV=production expo run:android --device --variant release",
    "build:android:aab": "cd android && ./gradlew bundleRelease",
    "prebuild:android": "expo prebuild --platform android --clean",
    "build:ios": "NODE_ENV=production expo run:ios --device --configuration Release",
    "build:web": "NODE_ENV=production expo export --platform web"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
